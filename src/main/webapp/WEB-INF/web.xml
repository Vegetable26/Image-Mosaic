<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
<!-- <web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"> -->


        <servlet>
            <servlet-name>ImageCollage</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.ImageCollageServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>upload</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.UploadServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>crawl</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.CrawlerServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>serve</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.ServeServlet</servlet-class>
        </servlet>

        <servlet-mapping>
            <servlet-name>ImageCollage</servlet-name>
            <url-pattern>/ImageCollage</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>upload</servlet-name>
            <url-pattern>/upload</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>serve</servlet-name>
            <url-pattern>/serve</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>crawl</servlet-name>
            <url-pattern>/crawl</url-pattern>
        </servlet-mapping>

    <welcome-file-list>
            <welcome-file>ImageCollage.jsp</welcome-file>
        </welcome-file-list>

    </web-app>
